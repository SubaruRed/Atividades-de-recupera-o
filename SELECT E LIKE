34. SELECT * FROM Customers WHERE CustomerName LIKE 'B%' ORDER BY CustomerID ASC;

35. SELECT * FROM Customers WHERE City LIKE '%o' ORDER BY Country;

36. SELECT * FROM Customers WHERE Country = 'Brazil' ORDER BY PostalCode, CustomerName;

37. SELECT * FROM Customers WHERE PostalCode LIKE '9%' ORDER BY CustomerName DESC;

38. SELECT * FROM Customers WHERE CustomerName LIKE '%Services%' ORDER BY ContactName;

39. SELECT * FROM Customers WHERE Country = 'Spain' ORDER BY City;

40. SELECT * FROM Customers WHERE City LIKE 'A%' ORDER BY CustomerName, Country;

