1. SELECT * FROM Customers ORDER BY CustomerName ASC;

2. SELECT * FROM Customers ORDER BY City, CustomerName;

3. SELECT * FROM Customers WHERE Country = 'France' ORDER BY PostalCode;

4. SELECT * FROM Customers ORDER BY Country, City;

5. SELECT * FROM Customers WHERE City = 'Berlin' ORDER BY CustomerName;

6. SELECT * FROM Customers WHERE Country = 'USA' ORDER BY City DESC;

7. SELECT * FROM Customers WHERE ContactName LIKE 'A%' ORDER BY CustomerID;

8. SELECT * FROM Customers WHERE Country != 'Canada' ORDER BY PostalCode;

9. SELECT * FROM Customers WHERE CustomerName LIKE '%Shop%' ORDER BY Country;

10. SELECT * FROM Customers WHERE Country = 'Mexico' ORDER BY CustomerID DESC;

11. SELECT * FROM Customers WHERE PostalCode LIKE '1%' ORDER BY CustomerName;

12. SELECT * FROM Customers WHERE City = 'Madrid' ORDER BY ContactName ASC;

13. SELECT * FROM Customers WHERE City LIKE 'P%' ORDER BY Country, CustomerID;

14. SELECT * FROM Customers WHERE City = 'New York' ORDER BY CustomerName;

15. SELECT * FROM Customers WHERE Country LIKE '%a' ORDER BY City ASC;

16. SELECT * FROM Customers WHERE PostalCode LIKE '5%' ORDER BY ContactName DESC;

17. SELECT * FROM Customers ORDER BY PostalCode DESC;

18. SELECT * FROM Customers WHERE CustomerName LIKE '%Ltd' ORDER BY Country, City;

19. SELECT * FROM Customers WHERE Country = 'Germany' ORDER BY CustomerName;

20. SELECT * FROM Customers WHERE ContactName LIKE '%son%' ORDER BY PostalCode;

21. SELECT * FROM Customers WHERE City = 'Tokyo' ORDER BY CustomerName DESC;

22. SELECT * FROM Customers WHERE PostalCode LIKE '%00' ORDER BY CustomerID;

23. SELECT * FROM Customers WHERE City = 'Paris' ORDER BY Country ASC;

24. SELECT * FROM Customers WHERE City LIKE 'S%' ORDER BY CustomerName;

25. SELECT * FROM Customers WHERE ContactName LIKE 'J%' ORDER BY Country, PostalCode;

26. SELECT * FROM Customers WHERE City != 'London' ORDER BY CustomerID;

27. SELECT * FROM Customers ORDER BY ContactName ASC;

28. SELECT * FROM Customers WHERE Country = 'Canada' ORDER BY PostalCode, CustomerName;

29. SELECT * FROM Customers WHERE CustomerName LIKE '%Global%' ORDER BY Country;

30. SELECT * FROM Customers WHERE Country LIKE 'I%' ORDER BY City;

31. SELECT * FROM Customers WHERE CustomerName LIKE 'T%' ORDER BY ContactName DESC;

32. SELECT * FROM Customers WHERE City = 'Chicago' ORDER BY PostalCode;

